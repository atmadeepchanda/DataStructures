t=int(input())
for _ in range(t):
    n=int(input())
    arr=list(map(int,input().split()))
    m=arr[0]
    sum=arr[0]
    for i in range(1,n):
        newsum=sum+arr[i]
        if newsum>=sum:
            sum=newsum
            m=max(m,newsum)
            if arr[i]>newsum:
                m=max(arr[i],m)
                sum=arr[i]
        else:
            sum=newsum
    print(m)
            
                
                
